      { "from": { "moduleId": "ctrl-1", "portId": "gate-out" }, "to": { "moduleId": "snare-1", "portId": "trigger" }, "kind": "gate" },
      { "from": { "moduleId": "ctrl-1", "portId": "gate-out" }, "to": { "moduleId": "hh-1", "portId": "trigger" }, "kind": "gate" }
