{
  "version": 1,
  "id": "slew-test",
  "name": "Slew Glide",
  "description": "Portamento test using Slew on pitch CV.",
  "graph": {
    "modules": [
      {
        "id": "lfo-1",
        "type": "lfo",
        "name": "Step LFO",
        "position": { "x": 120, "y": 120 },
        "params": { "rate": 0.6, "depth": 1, "offset": 0, "shape": "square", "bipolar": false }
      },
      {
        "id": "slew-1",
        "type": "slew",
        "name": "Slew",
        "position": { "x": 320, "y": 120 },
        "params": { "rise": 0.25, "fall": 0.25 }
      },
      {
        "id": "osc-1",
        "type": "oscillator",
        "name": "VCO",
        "position": { "x": 520, "y": 120 },
        "params": {
          "frequency": 110,
          "type": "sawtooth",
          "pwm": 0.5,
          "unison": 1,
          "detune": 0,
          "subMix": 0,
          "subOct": 1
        }
      },
      {
        "id": "gain-1",
        "type": "gain",
        "name": "VCA",
        "position": { "x": 720, "y": 120 },
        "params": { "gain": 0.7 }
      },
      {
        "id": "out-1",
        "type": "output",
        "name": "Main Out",
        "position": { "x": 920, "y": 120 },
        "params": { "level": 0.8 }
      }
    ],
    "connections": [
      { "from": { "moduleId": "lfo-1", "portId": "cv-out" }, "to": { "moduleId": "slew-1", "portId": "in" }, "kind": "cv" },
      { "from": { "moduleId": "slew-1", "portId": "out" }, "to": { "moduleId": "osc-1", "portId": "pitch" }, "kind": "cv" },
      { "from": { "moduleId": "osc-1", "portId": "out" }, "to": { "moduleId": "gain-1", "portId": "in" }, "kind": "audio" },
      { "from": { "moduleId": "gain-1", "portId": "out" }, "to": { "moduleId": "out-1", "portId": "in" }, "kind": "audio" }
    ]
  }
}
