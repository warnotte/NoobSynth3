        "type": "oscillator",
        "name": "VCO",
        "position": { "x": 0, "y": 0 },
        "params": {
          "frequency": 110,
          "type": "sawtooth",
          "unison": 1
        }
      },
      {
        "id": "adsr-1",
        "type": "adsr",
        "name": "Amp ADSR",
        "position": { "x": 0, "y": 0 },
